  },
  "scripts": {
    "clean": "rimraf ./dist ./tsconfig.tsbuildinfo"
  },
  "dependencies": {
    "@zilliqa-js/account": "workspace:*",
    "@zilliqa-js/blockchain": "workspace:*",
    "@zilliqa-js/contract": "workspace:*",
    "@zilliqa-js/proto": "workspace:*",
    "@zilliqa-js/core": "workspace:*",
    "@zilliqa-js/crypto": "workspace:*",
    "@zilliqa-js/subscriptions": "workspace:*",
    "@zilliqa-js/util": "workspace:*",
    "tslib": "2.3.1",
    "@types/bn.js": "5.1.0",
    "@types/camelcase": "4.1.0",
    "@types/long": "4.0.2",
    "@types/node": "18.7.18",
    "@types/websocket": "1.0.4"
  },
  "files": [
    "dist",
    "node_modules",
    "package.json",
    "LICENSE",
    "CHANGELOG.md",
    "README.md"

{
  "name": "@zilliqa-js/zilliqa",
  "version": "3.4.0-rc.0",
  "description": "Main entry point to the Zilliqa JS client.",
  "maintainers": [
    "Zilliqa Usability Team <maintainers@zilliqa.com> (https://github.com/zilliqa/)"
  ],
  "main": "dist/index.js",
  "node": "dist/index.js",
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "repository": "https://www.github.com/Zilliqa/zilliqa-js",
  "bugs": {
    "url": "https://github.com/Zilliqa/zilliqa-js/issues"
  },
  "scripts": {
    "clean": "rimraf lib",
    "build": "pnpm clean && pnpm build:ts && pnpm build:web",

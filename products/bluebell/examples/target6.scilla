scilla_version 0

library HelloWorld
type Bool = 
  | True
  | False



contract HelloWorld()


field welcome_msg : Uint64 = Uint64 0

transition setHello (msg : Uint64)
  is_owner = False;
  match is_owner with
  | True =>
    x = builtin print__impl msg
  | False =>
    x = builtin print__impl msg;
    y = builtin print__impl msg
  end
end

scilla_version 0

library HelloWorld
type Bool = 
  | True
  | False



contract HelloWorld()


field welcome_msg : Uint64 = Uint64 0

transition setHello (msg : Uint64)
  is_owner = True;
  match is_owner with
  | True =>
    x = builtin print msg
(*  | False =>
    welcome_msg := msg    *)
  end
end
